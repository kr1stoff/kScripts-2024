/* -------------------------------- 
Primary style
-------------------------------- */
body {
	margin: 0px;
	padding: 0px;
	font-size: 14px;
	font-family: "Microsoft YaHei","微软雅黑","雅黑宋体","新宋体","宋体","Microsoft JhengHei","华文细黑","STHeiti","MingLiu";
	background-color: #FFFFFF;
}

#header_banner {
	height: 90px;
	width:100%;
	border-bottom: 5px #009933 solid;
	position:fixed;
	left:0;
	top:0;
	z-index:99999999;
	background:#ffffff;
}

#banner_logo {
	max-width: 170px;
	width: 15%;
	height: 80px;
	margin-left:20px;
	background: url(../image/genedenovo_logo.png) no-repeat left top;
	background-size: 100% 80px;
	float: left;
}

#banner_title {
	width: 80%;
	text-align: center;
	font-weight: bold;
	font-size: 24px;
	margin-top: 22px;
	white-space: nowrap;
}

#banner_title span {
	color: #009933;
	font-size: 28px;
}

